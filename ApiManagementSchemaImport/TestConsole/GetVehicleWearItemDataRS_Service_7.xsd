              <xsd:documentation>Bank Name of the credit card.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Status" type="NameCodeDescriptionType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Card Status of the credit card.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
