        <xsd:sequence>
          <xsd:element name="TickerSymbol" type="oagis:StringType" minOccurs="0"/>
          <xsd:element name="CompanyName" type="oagis:StringType" minOccurs="0"/>
          <xsd:element name="Status" type="oagis:StringType" minOccurs="0"/>
          <xsd:element name="ExchangeName" type="oagis:StringType" minOccurs="0"/>
          <xsd:element name="OpenPrice" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="ClosePrice" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="NetChange" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="Volume" type="oagis:DoubleNumericType" minOccurs="0"/>
          <xsd:element name="HighPrice" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="LowPrice" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="DateTime" type="oagis:DateTimeType" minOccurs="0"/>
          <xsd:element name="PeRatio" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="YearHigh" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="YearLow" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="MarketClosedIndicator" type="oagis:IndicatorType" minOccurs="0"/>
          <xsd:element name="LastPrice" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="NetValue" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="NetAssetValue" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="EPSRatio" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="MarketCap" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="AvgVolume" type="oagis:DoubleNumericType" minOccurs="0"/>
          <xsd:element name="Share" type="oagis:ShortNumericType" minOccurs="0"/>
          <xsd:element name="BetaRatio" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="DayGain" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="CostBasis" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="MarketValue" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="Gain" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="PercentGain" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="OverallReturn" type="oagis:AmountType" minOccurs="0"/>
          <xsd:element name="Transaction" type="TransactionType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TransactionType">
    <xsd:sequence>
      <xsd:element name="Type" type="oagis:StringType" minOccurs="0"/>
      <xsd:element name="DateTime" type="oagis:DateTimeType" minOccurs="0"/>
      <xsd:element name="Share" type="oagis:ShortNumericType" minOccurs="0"/>
      <xsd:element name="Price" type="oagis:AmountType" minOccurs="0"/>
      <xsd:element name="Commission" type="oagis:AmountType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="ExternalReference" type="ExternalReferenceType"/>
  <xsd:complexType name="ExternalReferenceType">
    <xsd:sequence>
      <xsd:element name="ID" type="oagis:IdentifierType" nillable="false" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PartyID" type="oagis:IdentifierType" nillable="false" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="EventData" type="EventDataType"/>
  <xsd:complexType name="EventDataType">
    <xsd:sequence>
      <xsd:element name="EventAction" type="oagis:StringType"/>
      <xsd:element name="Event" type="oagis:StringType" minOccurs="0"/>
      <xsd:element name="IgnoreMinDayRuleIndicator" type="oagis:IndicatorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Indicates if the RTE Email process should ignore
						the minimum day rule checking for processing an email message
						event.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DeliveryChannel" type="oagis:StringType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="EventDataIdentifier" type="EventDataIdentifierType"/>
  <xsd:complexType name="EventDataIdentifierType">
    <xsd:sequence>
      <xsd:element ref="AccountID" minOccurs="0"/>
      <xsd:element ref="VehicleID" minOccurs="0"/>
      <xsd:element ref="SubscriberID" minOccurs="0"/>
      <xsd:element name="SubscriberEmail" type="oagis:StringType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="MessageID" type="oagis:IdentifierType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="StartDate" type="oagis:DateTimeType" minOccurs="0"/>
      <xsd:element name="EndDate" type="oagis:DateTimeType" minOccurs="0"/>
      <xsd:element name="DealerCode" type="oagis:CodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DivisionCode" type="oagis:CodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BusinessAssociateCode" type="oagis:CodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="VehicleWearItemData" type="VehicleWearItemDataType"/>
  <xsd:complexType name="VehicleWearItemDataType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element name="Vehicle" type="star:VehicleABIEType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Vehicle for Make, Model, Year, and
								Manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CurrentMileage" type="sqdt:LengthMeasureType" minOccurs="0"/>
          <xsd:element name="WearItemData" type="WearItemDataType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Wear Item Data like Wear Item, Mileage, Offset Min Value, Offset Max
								Value</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WearItemDataType">
    <xsd:complexContent>
      <xsd:extension base="star:PartsProductItemType">
        <xsd:sequence>
          <xsd:element name="WearItemMileage" type="sqdt:LengthMeasureType" minOccurs="0"/>
          <xsd:element name="WearItemOffsetMinValue" type="sqdt:LengthMeasureType" minOccurs="0"/>
          <xsd:element name="WearItemOffsetMaxValue" type="sqdt:LengthMeasureType" minOccurs="0"/>
          <xsd:element name="WearItemURL" type="UrlResourceType" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="MaintenanceCompletedFlag" type="oagis:IndicatorType" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="VehicleWarrantyData" type="VehicleWarrantyDataType"/>
  <xsd:complexType name="VehicleWarrantyDataType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element ref="VehicleID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A unique Vehicle Identification Number
								provided for a vehicle. An intelligent code that uniquely
								identifies a Vehicle. Example Data: 1G1AW37V9LR005234;
								3CS44E1JN313584; 2G1AN69HXD1258661</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="WarrantyBlock" type="WarrantyBlockType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Vehicle Warranty Block Code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="WarrantyData" type="WarrantyDataType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Vehicle Warranty Data like Warranty Code, Description, Start Date,
								Start Mileage, Exp Date, Exp Mileage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WarrantyBlockType">
    <xsd:sequence>
      <xsd:element name="WarrantyBlockType" type="NameCodeDescriptionType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Warranty Code, Name , Description</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="WarrantyBlockDate" type="oagis:DateTimeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Date on which the warranty block became effective</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="WarrantyBlockLaborOperation" type="star:ServiceLaborHistoryType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Description of blocked labor operations</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="WarrantyBlockTransactionType" type="oagis:StringType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Blocked transaction types</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="WarrantyDataType">
    <xsd:complexContent>
      <xsd:extension base="star:RemarketingWarrantyType">
        <xsd:annotation>
          <xsd:documentation>Vehicle Warranty Base Type</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="WarrantyStatus" type="oagis:TextType" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="RecallCampaign" type="RecallCampaignType"/>
  <xsd:complexType name="RecallCampaignType">
    <xsd:complexContent>
      <xsd:extension base="NameCodeDescriptionType">
        <xsd:sequence>
          <xsd:element name="DisplayText" type="oagis:DescriptionType" minOccurs="0"/>
          <xsd:element name="CampaignDate" type="oagis:DateType" minOccurs="0"/>
          <xsd:element name="URL" type="UrlResourceType" minOccurs="0"/>
          <xsd:element name="EmergencyIndicator" type="oagis:IndicatorType" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="VehicleProfile" type="VehicleProfileType"/>
  <xsd:complexType name="VehicleProfileType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element ref="AccountID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unique number identifying the account</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="VehicleID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A unique Vehicle Identification Number
								provided for a vehicle. An intelligent code that uniquely
								identifies a Vehicle. Example Data: 1G1AW37V9LR005234;
								3CS44E1JN313584; 2G1AN69HXD1258661</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="StationID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The StationID of the vehicle's Unit.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="VehicleStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of valid STATUS_CD for a given
								VEHICLE. INACTIVE, ACTIVE, IN-SETUP, CANCELLED.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AccountVehicleStartDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date a vehicle was associated to the
								account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="License" type="VehicleLicenseType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The license plate number of the vehicle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DeliveryDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date the vehicle was delivered by a dealer
								to a subscriber.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UsedVehicleIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if the vehicle
								is used.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SoldDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date a vehicle was sold to a subscriber by
								a selling dealer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BBWCIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>First Call Indicator. An indicator used to
								determine if the Blue Button Welcome Call has been completed for
								a vehicle. Values are: Y = first call completed, N = first call
								not completed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BBWCDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date Blue Button Welcome Call (first call)
								completed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Tenure" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The tenure for the vehicle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GMFamilyFirstIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								is a GM Family First Customer, or not. Values are: True = GM
								Family First Cust, False = Not a GM Family First Cust.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="StatusReason" type="NameCodeDescriptionType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Provide multiple reasons for scenarios
								including Cancel, Decline, Deactivate, Nuisance. Use name, code,
								and descriptions to capture appropriate reason details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SalesPerson" type="OnStarPersonType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Sales Person who sold the vehicle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SoldByDealer" type="DealerType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PreferredDealer" type="DealerType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GarageAddress" type="AddressType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Garage Address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NuisanceVehicleIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a vehicle is
								a nuisance vehicle or not.Values are: True = nuisance vehicle,
								False = not a nuisance vehicle. Note: A nuisance vehicle
								generally has a sensitive AACN sensor and calls into the Call
								Center looking like an Emergency Call, however there is no
								emergency â the sensor is erroneously calling in.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NuisanceVehicleFollowUpDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date that OnStar contacted subscriber to
								ensure that a nuisance vehicle issue is remedied. Note:
								Depending on the problem OnStar may recommend that the
								subscriber take their vehicle to a dealer, or OnStar may just
								pass the case onto OSTA. Once OnStar performs the follow-up and
								the problem is remedied, the nuisance vehicle iflag is
								turned-off (set to N.)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="RoadSideAssistanceProvider" type="RoadSideAssistanceProviderType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Insurance" type="InsuranceType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PSAPColorName" type="oagis:NameType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Vehicle driver's preferred description for a
								vehicle's make, model, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="VehicleSubscribers" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the list of Subscriber information
								associated with the vehicle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BillingVehicle" type="BillingVehicleType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Vehicle details for Billing.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AdditionalInfo" type="NameValueType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EventUsage" type="EventUsageType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Details related event usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NickName" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GracePeriodDate" type="oagis:DateTimeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UFRDate" type="oagis:DateTimeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>This is the up for renewal date on the base
								product for a given vehicle.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="OdometerReading" type="OdometerReadingType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Odometer reading captured for Aftermarket vehicles</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Retailer" type="RetailerType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Retailer information for Aftermarket vehicles</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Alert" type="AlertType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlertType">
    <xsd:complexContent>
      <xsd:extension base="NameCodeDescriptionType">
        <xsd:sequence>
          <xsd:element name="Status" type="oagis:IndicatorType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DealerType">
    <xsd:complexContent>
      <xsd:extension base="star:PartyABIEType">
        <xsd:sequence>
          <xsd:element name="ID" type="oagis:IdentifierType" minOccurs="0"/>
          <xsd:element name="DealerCode" type="oagis:CodeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BusinessAssociateCode" type="oagis:CodeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SellingSourceCode" type="oagis:CodeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DMNSupportIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indictes if the dealer supports the DMN
								program.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Url" type="UrlResourceType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Support multiple Urls for Dealer Marketing
								content. Url Name will be utilized for identifying the type
								(i.e. 'Dealer', 'DealerRequest', 'Coupon')</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ActiveIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if the dealer
								is currently active.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="Address" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Includes the physical and mailing address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RoadSideAssistanceProviderType">
    <xsd:complexContent>
      <xsd:extension base="star:PartyABIEType">
        <xsd:sequence>
          <xsd:element name="RoadSideAssistanceProviderName" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the roadside assitance provider</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PolicyNumberText" type="oagis:IdentifierType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A unique identification number assigned to a
								roadside assistance insurance policy.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="roadSideAssistanceProviderType" type="RoadSideAssistanceProviderTypeEnum">
          <xsd:annotation>
            <xsd:documentation>The list of valid road side assistance provider
							type. PRIMARY, SECONDARY</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="RoadSideAssistanceProviderTypeEnum">
    <xsd:restriction base="oagis:StringType">
      <xsd:enumeration value="PRIMARY"/>
      <xsd:enumeration value="SECONDARY"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="InsuranceType">
    <xsd:complexContent>
      <xsd:extension base="star:InsuranceType">
        <xsd:sequence>
          <xsd:element name="VehicleInsuranceID" type="oagis:IdentifierType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Code that represents the Insurance Type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="InsuranceType" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Code that represents the Insurance Type</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="VehicleSubscribers" type="VehicleSubscribersType"/>
  <xsd:complexType name="VehicleSubscribersType">
    <xsd:complexContent>
      <xsd:extension base="ListElementType">
        <xsd:sequence>
          <xsd:element ref="SubscriberList" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The list of Subscribers.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="BillingVehicleType">
    <xsd:annotation>
      <xsd:documentation>The BillingVehicle type.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="star:VehicleABIEType">
        <xsd:sequence>
          <xsd:element name="EffectiveDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The effective date for the billing transaction
								(cancel, update, and transfer).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UFRDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Up For Renewal Date.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="EventUsageType">
    <xsd:annotation>
      <xsd:documentation>Details for event usage for billing update.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="EventClassCode" type="oagis:CodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Event class code. Valid values: 'Usage'</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EventTypeCode" type="oagis:CodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Event type code. Valid values: 'Prepaid Usage'</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EventSubTypeCode" type="oagis:CodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Event subtype code. Valid value: 'Limited Route'</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SwitchDate" type="oagis:DateType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The event usage date.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Volume" type="QuantityType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Specifies the number by which the duartion of a
						product can be extended by an offer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AdditionalInfo" type="NameValueType" nillable="true" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Additional information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="SubscriberList" type="SubscriberListType"/>
  <xsd:complexType name="SubscriberListType">
    <xsd:sequence>
      <xsd:element ref="SubscriberID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Subscriber ID.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrimaryDriverIndicator" type="oagis:IndicatorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An indicator used to identify the subscriber is
						the primary driver of the vehicle.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!--Start of New Offer Proposal-->
  <xsd:element name="Offers" type="OffersType"/>
  <xsd:element name="Products" type="ProductsType"/>
  <xsd:element name="Features" type="FeaturesType"/>
  <xsd:element name="Value" type="xsd:string"/>
  <xsd:element name="Type" type="xsd:string"/>
  <xsd:element name="Price" type="PriceType"/>
  <xsd:element name="validFor" type="TimePeriodType"/>
  <xsd:element name="Discounts" type="DiscountsType"/>
  <xsd:complexType name="OffersType">
    <xsd:sequence>
      <xsd:element name="Offer" type="OfferType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OfferType">
    <xsd:sequence>
      <xsd:element ref="ID" minOccurs="0"/>
      <xsd:element ref="Name" minOccurs="0"/>
      <xsd:element ref="Description" minOccurs="0"/>
      <xsd:element ref="Type" minOccurs="0"/>
      <xsd:element ref="Products" minOccurs="0"/>
      <xsd:element ref="validFor" minOccurs="0"/>
      <xsd:element name="OfferCharacteristic" type="OfferCharacteristicType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DiscountsType">
    <xsd:sequence>
      <xsd:element name="Discount" type="DiscountType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <!--Added as part of R5.3 P&P-->
  <xsd:complexType name="ProdComponentsType">
    <xsd:sequence>
      <xsd:element name="ProdComponent" type="ProdComponentType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ProdComponentType">
    <xsd:sequence>
      <xsd:element name="ComponentID" minOccurs="0"/>
      <xsd:element ref="Description" minOccurs="0"/>
      <xsd:element ref="Price" minOccurs="0"/>
      <xsd:element ref="Discounts" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <!--Added as part of R5.3 P&P-->
  <xsd:complexType name="DiscountType">
    <xsd:sequence>
      <xsd:element ref="ID" minOccurs="0"/>
      <xsd:element ref="Name" minOccurs="0"/>
      <xsd:element ref="validFor" minOccurs="0"/>
      <xsd:element name="Price" type="PriceType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OfferCharacteristicType">
    <xsd:sequence>
      <xsd:element name="UpsellOfferingID" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ShortDescription" type="xsd:string" minOccurs="0"/>
      <xsd:element name="HFCOperationType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Rank" type="RankType" minOccurs="0"/>
      <xsd:element name="Classification" type="xsd:string" minOccurs="0"/>
      <xsd:element name="PaymentMethodOnFileRequired" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="ClassificationDescription" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="RankType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="type" type="xsd:string" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ProductsType">
    <xsd:sequence>
      <xsd:element name="Product" type="NewProductType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="NewProductType">
    <xsd:annotation>
      <xsd:documentation>Use ID for Product Code (currently Product -> ProductSpecification -> productNumber) - CORE, HFC, LTUSG</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="ID" minOccurs="0"/>
      <xsd:element ref="Name" minOccurs="0"/>
      <xsd:element ref="Description" minOccurs="0"/>
      <xsd:element ref="validFor" minOccurs="0"/>
      <!--Added as part of R5.3 P&P-->
      <xsd:element ref="Type" minOccurs="0"/>
      <xsd:element ref="Price" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="ProdComponents" type="ProdComponentsType" minOccurs="0" maxOccurs="1"/>
      <xsd:element ref="Discounts" minOccurs="0"/>
      <xsd:element name="Taxes" type="TaxType" minOccurs="0"/>
      <xsd:element name="Features" type="FeaturesType" minOccurs="0"/>
      <!--Added as part of R5.3 P&P-->
      <xsd:element name="ProductCharacteristic" type="ProductCharacteristicType" minOccurs="0"/>
      <xsd:element name="ProductOfferCharacteristic" type="ProductOfferCharacteristicType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ProductCharacteristicType">
    <xsd:sequence>
      <xsd:element name="ServiceOrderCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="CarrierDataShareGroup" type="xsd:string" minOccurs="0"/>
      <!--Added as part of R5.3 P&P-->
      <xsd:element name="ExternalOrderNumber" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ExternalProductInstanceID" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ExternalChangeType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Seller" type="xsd:string" minOccurs="0"/>
      <xsd:element name="HFCOperationType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="BundleLineNum" type="xsd:string" minOccurs="0"/>
      <xsd:element name="SequenceNumber" type="xsd:string" minOccurs="0"/>
      <xsd:element name="PricePlan" type="xsd:string" minOccurs="0"/>
      <!--Added as part of R5.3 P&P-->
      <xsd:element name="IsTrial" type="xsd:boolean" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <!--Added as part of R5.3 P&P-->
  <xsd:complexType name="TaxType">
    <xsd:sequence>
      <xsd:element name="Tax" type="oagis:TaxAmountType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <!--Added as part of R5.3 P&P-->
  <xsd:complexType name="ProductOfferCharacteristicType">
    <xsd:sequence>
      <xsd:element name="Sequence" type="xsd:string" minOccurs="0"/>
      <xsd:element name="BundleLineNum" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Duration" type="DurationType" minOccurs="0"/>
      <xsd:element name="PricePlan" type="ProgramCharacteristicType" minOccurs="0"/>
      <xsd:element name="MonthlyPrice" type="PriceType" minOccurs="0"/>
      <xsd:element name="AnnualPrice" type="PriceType" minOccurs="0"/>
      <xsd:element name="OverageAmount" type="PriceType" minOccurs="0"/>
      <xsd:element name="ServiceQuantity" type="UsageType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UsageType">
    <xsd:sequence>
      <xsd:element ref="Value" minOccurs="0"/>
      <xsd:element name="UnitofMeasure" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DurationType">
    <xsd:sequence>
      <xsd:element ref="Type" minOccurs="0"/>
      <xsd:element name="DurationUpperLimit" type="xsd:duration" minOccurs="0"/>
      <xsd:element name="DurationLowerLimit" type="xsd:duration" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TimePeriodType">
    <xsd:sequence>
      <xsd:element name="startDateTime" type="xsd:dateTime" minOccurs="0"/>
      <xsd:element name="endDateTime" type="xsd:dateTime" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PriceType">
    <xsd:sequence>
      <xsd:element name="Amount" type="xsd:double" minOccurs="0"/>
      <xsd:element name="CurrencyCode" type="xsd:string" minOccurs="0"/>
      <xsd:element ref="Type" minOccurs="0"/>
      <xsd:element ref="validFor" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <!--End of New Offer Proposal-->
  <xsd:element name="OnStarBundledProductOffering" type="OnStarBundledProductOfferingType"/>
  <xsd:complexType name="OnStarBundledProductOfferingType">
    <xsd:annotation>
      <xsd:documentation>Specifies information about the offerings
				associated with the products.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="SIDBusPr:BundledProductOffering">
        <xsd:sequence>
          <xsd:element name="Characteristic" type="ProgramCharacteristicType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Product" type="ProductType"/>
  <xsd:complexType name="ProductType">
    <xsd:annotation>
      <xsd:documentation>Specifies information about the products that are
				effective on a vehicle over time</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="SIDBusPr:Product">
        <xsd:sequence>
          <xsd:element ref="OnStarProductOffering" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="OnStarProductOffering" type="OnStarProductOfferingType"/>
  <xsd:complexType name="OnStarProductOfferingType">
    <xsd:annotation>
      <xsd:documentation>Specifies information about the offers associated
				with the products.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="SIDBusPr:ProductOffering">
        <xsd:sequence>
          <xsd:element name="Characteristic" type="ProgramCharacteristicType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ProgramCharacteristicType">
    <xsd:complexContent>
      <xsd:extension base="NameCodeDescriptionType">
        <xsd:sequence>
          <xsd:element name="Value" type="ValueType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The value for this characteristic.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="MaintenanceRecords" type="MaintenanceRecordsType"/>
  <xsd:complexType name="MaintenanceRecordsType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element ref="VehicleID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A unique Vehicle Identification Number
								provided for a vehicle. An intelligent code that uniquely
								identifies a Vehicle. Example Data: 1G1AW37V9LR005234;
								3CS44E1JN313584; 2G1AN69HXD1258661</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AccountID" type="oagis:IdentifierType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The account number an OnStar account is
								identified by.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="StartDate" type="oagis:DateTimeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maintenance Service start date.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EndDate" type="oagis:DateTimeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maintenance Service end date.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ShowDealerDataIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicate whether to display dealer data or not</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="MaintenanceRecordsData" type="MaintenanceRecordsDataType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Vehicle Maintenance Service data</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MaintenanceRecordsDataType">
    <xsd:complexContent>
      <xsd:extension base="star:InServiceType">
        <xsd:sequence>
          <xsd:element name="ServicePlace" type="oagis:TextType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Dealer place where service was done.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SourceOfData" type="ServiceSourceOfData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Source of Service data e.g. Subscriber, DPP,
								DD.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CostOfService" type="oagis:AmountType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Total cost for service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Comment" type="oagis:TextType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Comments from subscriber.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="ServiceSourceOfData">
    <xsd:restriction base="oagis:StringType">
      <xsd:enumeration value="Subscriber_Manual">
        <xsd:annotation>
          <xsd:documentation>Manual service data entry by Subscriber</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="US_Dealer_Feed">
        <xsd:annotation>
          <xsd:documentation>Service data feed from US Dealer application</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CA_Dealer_Feed">
        <xsd:annotation>
          <xsd:documentation>Service data feed from Canada Dealer application</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="UrlResource" type="UrlResourceType"/>
  <xsd:complexType name="UrlResourceType">
    <xsd:complexContent>
      <xsd:extension base="UrlResourceBaseType">
        <xsd:attribute name="languageID" type="oagis:NormalizedStringType">
          <xsd:annotation>
            <xsd:documentation>Indicates the language-country associated with the Url Resource.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="UrlResourceBaseType">
    <xsd:sequence>
      <xsd:element name="ResourceType" type="UrlResourceTypeEnum" nillable="true" minOccurs="0"/>
      <xsd:element name="ImageType" type="ImageFormatContentEnum" nillable="true" minOccurs="0"/>
      <xsd:element name="Name" type="oagis:NameType"/>
      <xsd:element name="URL" type="oagis:URIType"/>
      <xsd:element name="ImageContentType" type="CodeDescriptionType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>- A code to represent the Image Content Type
						- A desc to represent the Image Content Type (Front Seat Image,
						Marketing Color Image, etc.)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Description" type="oagis:DescriptionType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Url Text Description.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="UrlResourceTypeEnum">
    <xsd:restriction base="oagis:StringType">
      <xsd:enumeration value="WEB_PAGE"/>
      <xsd:enumeration value="IMAGE"/>
      <xsd:enumeration value="PDF"/>
      <xsd:enumeration value="TEXT"/>
      <xsd:enumeration value="UNKNOWN"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ImageFormatContentEnum">
    <xsd:annotation>
      <xsd:documentation>This code list represents Image File Formats.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:normalizedString">
      <xsd:enumeration value="jpeg"/>
      <xsd:enumeration value="tiff"/>
      <xsd:enumeration value="raw"/>
      <xsd:enumeration value="png"/>
      <xsd:enumeration value="gif"/>
      <xsd:enumeration value="bmp"/>
      <xsd:enumeration value="ppm"/>
      <xsd:enumeration value="pgm"/>
      <xsd:enumeration value="pbm"/>
      <xsd:enumeration value="pnm"/>
      <xsd:enumeration value="cgm"/>
      <xsd:enumeration value="svg"/>
      <xsd:enumeration value="other"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="NameIDDescriptionType">
    <xsd:complexContent>
      <xsd:extension base="IDDescriptionType">
        <xsd:sequence>
          <xsd:element name="Name" type="oagis:NameType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="IDDescriptionType">
    <xsd:sequence>
      <xsd:element name="ID" type="oagis:IdentifierType"/>
      <xsd:element name="Description" type="oagis:DescriptionType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="QuantityType">
    <xsd:simpleContent>
      <xsd:extension base="oagis:MeasureType">
        <xsd:attribute name="uom" type="oagis:StringType" use="optional">
          <xsd:annotation>
            <xsd:documentation>The quantity of an item measured with a unit of measure.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="BaseNounType">
    <xsd:sequence>
      <xsd:element name="ID" type="oagis:IdentifierType" nillable="false" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="NameValueType">
    <xsd:annotation>
      <xsd:documentation>The name value type</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="oagis:StringType">
        <xsd:annotation>
          <xsd:documentation>The name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Value" type="ValueType" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The value type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ValueType">
    <xsd:simpleContent>
      <xsd:extension base="oagis:StringType">
        <xsd:attribute name="type" type="oagis:StringType" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="AccountType" type="NameCodeDescriptionType">
    <xsd:annotation>
      <xsd:documentation>-A unique code for a given account type e.g., P
				for PERSONAL, B for BUSINESS.
				-The name of the account type
				-The text
				description of the account type</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="AccountType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element ref="AccountID">
            <xsd:annotation>
              <xsd:documentation>The account number an OnStar account is
								identified by.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="StartDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date the account was activated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EndDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date the account was inactivated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CreatedDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date the account was created. This is
								different from the activation date, since it can be setup to be
								activated on a future date.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GracePeriodDate" type="oagis:DateTimeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="AccountType" minOccurs="0"/>
          <xsd:element ref="AccountStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Account Status. See AccountStatus for details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="StatusReason" type="NameCodeDescriptionType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Provide multiple reasons for scenarios
								including Cancel, Decline, Deactivate, Nuisance. Use name, code,
								and descriptions to capture appropriate reason details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PaymentMethodOnFileIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used for status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DeclinePaymentMethodIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator for declining to provide payment
								method information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CompensationAlertCode" type="oagis:CodeType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code assigned to an account based on
								analysis of history of providing compensations to the same
								account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PasswordOnlyIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if an account
								is a password only account. That is a password is required.
								Values are: True = password required, False = password not
								required.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NotificationIndicator" type="NameIndicatorType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if account
								subscribers want to receive notifications by Call, Mail, Email,
								etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="Address" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies the physical address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="InfoIndicator" type="NameIndicatorType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if there is any
								critical information missing on the account</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="AccountSubscribers" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the list of Subscriber IDs associated
								with the account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AccountVehicles" type="AccountVehiclesType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the list of Vehicle IDs associated
								with the account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="BillingAccount" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Includes the Billing Account Details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CarrierAccount" type="CarrierAccountType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Includes the Carrier Account Details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Organization" type="star:OrganizationABIEType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The fleet organization.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AdditionalInfo" type="NameValueType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DoNotCallIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a
								does not want to receive phone calls at their home from OnStar.
								Values are: True = don't call, False = can call.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DoNotMailIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								does not want to receive mail at their home from OnStar. Values
								are: True = don't mail, False = can mail.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DoNotEmailIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								does not want to receive Email from OnStar. Values are: True =
								don't Email, False = can Email.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="MissingCriticalInfoIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if there is any
								critical information missing on the account</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BillingDay" type="oagis:DayDateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The day of the month, this account is billed.
								Once the account is officially started, this cannot be changed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="BillingStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Account Billing Status . See
								AccountBillingStatus for details</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="CancelReason" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The reason for canceling the account. See
								CancelReason for details</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UpForRenewalDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date that an account is up for renewal. This
								date is also known as the Price Lock Through Date</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="CreditCard" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>credit card associated with the account</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="OrderInformation" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="BillingStatus" type="NameCodeDescriptionType">
    <xsd:annotation>
      <xsd:documentation>Specifies the valid types that can be used to
				classify an account's billing status. Examples include: currently
				paid, written off, past due, credit card pending, credit card
				denied, normal.
				- The code indicating the status of account billing.
				- The word or words that an account billing status type is known by.
				Examples include: currently paid, written off, past due, credit card
				pending, credit card denied, normal.
				- Additional information about
				an account billing status type.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="CancelReason" type="NameCodeDescriptionType">
    <xsd:annotation>
      <xsd:documentation>- The code for canceling the account
				- The name for
				canceling the account
				- The description for canceling the account</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="AccountID" type="oagis:IdentifierType">
    <xsd:annotation>
      <xsd:documentation>The account number an OnStar account is identified
				by.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="NameCodeDescriptionType">
    <xsd:complexContent>
      <xsd:extension base="CodeDescriptionType">
        <xsd:sequence>
          <xsd:element name="Name" type="oagis:NameType" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CodeDescriptionType">
    <xsd:sequence>
      <xsd:element name="Code" type="oagis:CodeType"/>
      <xsd:element name="Description" type="oagis:DescriptionType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="AccountStatus" type="NameCodeDescriptionType">
    <xsd:annotation>
      <xsd:documentation>-A unique code provided to this account status
				value
				-The name of the account status type
				-The text description of
				the account status type</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="NameIndicatorType">
    <xsd:annotation>
      <xsd:documentation>This entity includes an indicator value for each
				name.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="oagis:StringType" nillable="false">
        <xsd:annotation>
          <xsd:documentation>The name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Indicator" type="oagis:IndicatorType" nillable="false">
        <xsd:annotation>
          <xsd:documentation>An indicator used for the name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="Address" type="AddressType"/>
  <xsd:complexType name="AddressType">
    <xsd:complexContent>
      <xsd:extension base="star:AddressABIEType">
        <xsd:sequence>
          <xsd:element name="ZipCodePlus4" type="oagis:CodeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Zip code plus 4.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PreferredName" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Preferred name for an address (destination,)
								provided by a subscriber on the account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DownloadDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date an address (destination) was downloaded
								to a vehicle belonging to an account.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SavedDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date an address (destination) was saved by a
								subscriber</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PrimaryIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if an address
								for an account is the primary mailing address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SecondaryIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if an address
								for an account is the secondary mailing address</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="LocationID" type="oagis:IdentifierType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="AccountSubscribers" type="AccountSubscribersType"/>
  <xsd:complexType name="AccountSubscribersType">
    <xsd:complexContent>
      <xsd:extension base="ListElementType">
        <xsd:sequence>
          <xsd:element name="Subscriber" type="SubscriberType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The list of Subscribers.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ListElementType">
    <xsd:sequence>
      <xsd:choice minOccurs="0">
        <xsd:element ref="oagis:Get">
          <xsd:annotation>
            <xsd:documentation>The Get contains the attributes for Recordset
							managment on the request.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element ref="oagis:Show">
          <xsd:annotation>
            <xsd:documentation>The Show contains the attributes for Recordset
							managment on the response.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SubscriberType">
    <xsd:complexContent>
      <xsd:extension base="OnStarPersonType">
        <xsd:sequence>
          <xsd:element ref="SubscriberID">
            <xsd:annotation>
              <xsd:documentation>The subscriber ID.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Type" type="NameCodeDescriptionType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The subscriber ID.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SubscriberDriverLicenseNumber" type="oagis:IdentifierType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SubscriberPasswordText" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The word or words that identify a subscriber's
								security password. Business rules: minimum 8 characters;
								comprised of at least one number and one letter; letters are not
								case sensitive; store as a hashed value; mask input password.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EmailNotProvidedReason" type="NameCodeDescriptionType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								has opted in for email service, or not. This is for all emails
								from Onstar. Values are: True = Allow email, False = Don't allow
								email.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EmailOptInIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								has opted in for email service, or not. This is for all emails
								from Onstar. Values are: True = Allow email, False = Don't allow
								email.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EmailOptInDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date that a subscriber opts in or out of email
								service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GMFamilyFirstCustomerIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								is a GM Family First Customer, or not. Values are: True = GM
								Family First Cust, False = Not a GM Family First Cust.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PrimaryEmergencyContact" type="star:ContactABIEType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SecondaryEmergencyContact" type="star:ContactABIEType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SubscriberSinceDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date that a subscriber is enrolled.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SubscriberPasswordIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								password is the default password. Values are: True = current
								password is the default password, False = current password is
								not the default password.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ICEOptInIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								has chosen to either include or not include ICE data. Values
								are: True = Include ICE data, False = Don't include ICE data.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ICEOptInDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date that a subscriber opts in or out of email
								service.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EmergencyContact" type="OnStarPersonType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Emergency Contact details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="SubscriberAccounts" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the list of Account IDs associated
								with the subscriber.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="SubscriberVehicles" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the list of Vehicle information
								associated with the subscriber.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AdditionalInfo" type="NameValueType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional Subscriber preferences</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="VAProfileDomain" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contains Virtual Advisor domain information
								(weather, stock, traffic) and their correspoonding content.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="SubscriberRole" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies information about the roles that a
								subscriber may have in relationship to an account. For example,
								Account Owner, Subscriber, Owner Proxy.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DocumentAcceptance" type="DocumentAcceptanceType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="OnStarPersonType">
    <xsd:complexContent>
      <xsd:extension base="star:PersonType">
        <xsd:sequence>
          <xsd:element ref="NationalityID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The NationalityID to uniquely identify an individual such as
								GovernmentID, Driver Liscense Number.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PreferredLanguageCode" type="PreferredLanguageType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies the persons preferred language.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PersonType">
    <xsd:complexContent>
      <xsd:extension base="star:PersonType">
        <xsd:sequence>
          <xsd:element name="PreferredLanguageCode" type="PreferredLanguageType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies the persons preferred language.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="NationalityID" type="NationalityIDType"/>
  <xsd:complexType name="NationalityIDType">
    <xsd:annotation>
      <xsd:documentation>The NationalityID to uniquely identify an individual such as
				GovernmentID, Driver Liscense Number.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="oagis:IdentifierType">
        <xsd:attribute name="type" type="oagis:StringType" use="optional">
          <xsd:annotation>
            <xsd:documentation>Indicates the type of nationality id (i.e. GovernmentID, Drivers License.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="PreferredLanguageType">
    <xsd:simpleContent>
      <xsd:extension base="oagis:LanguageCodeType">
        <xsd:attribute name="type" type="oagis:NormalizedStringType">
          <xsd:annotation>
            <xsd:documentation source="http://www.openapplications.org/oagis/9">Indicate the type of the preferred language (primary, secondary).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="SubscriberID" type="oagis:IdentifierType">
    <xsd:annotation>
      <xsd:documentation>Represents the Subscriber Identifier</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="DocumentAcceptanceType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element name="Document" type="oagis:DocumentReferenceBaseType"/>
          <xsd:element name="AcceptanceIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine if a subscriber
								consented to the legal document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AcceptanceDateTime" type="oagis:DateTimeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date/time at which subscriber responded to
								the legal acceptance notice.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="MNOCustomerDetails" type="MNOCustomerDetailsType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Additional attributes optionally required by the
								various carriers in Europe when accepting Terms and Conditions.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MNOCustomerDetailsType">
    <xsd:annotation>
      <xsd:documentation>Additional Customer Information required as per MNO Legal/Regulatory requirements</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="mobileContactNumber" type="xsd:string" minOccurs="0"/>
      <xsd:element name="dateOfBirth" type="xsd:date" minOccurs="0"/>
      <xsd:element name="placeOfBirth" type="xsd:string" minOccurs="0"/>
      <xsd:element name="fiscalCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="secondSurname" type="xsd:string" minOccurs="0"/>
      <xsd:element name="idType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="idNumber" type="xsd:string" minOccurs="0"/>
      <xsd:element name="nationality" type="xsd:string" minOccurs="0"/>
      <xsd:element name="bankAccountNumber" type="xsd:string" minOccurs="0"/>
      <xsd:element name="patronymic" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="SubscriberAccounts" type="SubscriberAccountsType"/>
  <xsd:complexType name="SubscriberAccountsType">
    <xsd:complexContent>
      <xsd:extension base="ListElementType">
        <xsd:sequence>
          <xsd:element ref="SubscriberAccountList" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The list of Accounts and corresponding roles.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="SubscriberAccountList" type="SubscriberAccountListType"/>
  <xsd:complexType name="SubscriberAccountListType">
    <xsd:sequence>
      <xsd:element ref="AccountID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The list of Account IDs.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AccountSubscriberStatus" type="NameCodeDescriptionType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Account Subscriber Status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="SubscriberRole" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Specifies information about the roles that a
						subscriber may have in relationship to an account. For example,
						Account Owner, Subscriber, Owner Proxy.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="SubscriberRole" type="NameCodeDescriptionType">
    <xsd:annotation>
      <xsd:documentation>- Name: The word or words that an account
				subscriber's role is known by. For example, Account Owner,
				Subscriber, Owner Proxy.
				- Description: Additional information about
				an account subscriber role.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="VehicleList" type="VehicleListType"/>
  <xsd:complexType name="VehicleListType">
    <xsd:sequence>
      <xsd:element ref="VehicleID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The list of Vehicle IDs.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CreatedDate" type="oagis:DateType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The date this driver was associated with this
						vehicle.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PreferedDescription" type="oagis:DescriptionType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Vehicle driver's preferred description for a
						vehicle's make, model, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="SubscriberVehicles" type="SubscriberVehiclesType"/>
  <xsd:complexType name="SubscriberVehiclesType">
    <xsd:complexContent>
      <xsd:extension base="ListElementType">
        <xsd:sequence>
          <xsd:element ref="SubscriberVehicleList" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The list of Vehicle IDs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="SubscriberVehicleList" type="SubscriberVehicleListType"/>
  <xsd:complexType name="SubscriberVehicleListType">
    <xsd:sequence>
      <xsd:element ref="VehicleID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Vehicle ID.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="VehicleID" type="VehicleIdentifierType">
    <xsd:annotation>
      <xsd:documentation>Represents the Vehicle Identification Number (VIN)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="VehicleIdentifierType">
    <xsd:simpleContent>
      <xsd:restriction base="oagis:IdentifierType">
        <xsd:maxLength value="17"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="VAProfileDomain" type="VAProfileDomainType"/>
  <xsd:complexType name="VAProfileDomainType">
    <xsd:complexContent>
