  </xsd:complexType>
  <xsd:complexType name="ServicesType">
    <xsd:sequence>
      <xsd:element name="ServiceCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Service" type="ServiceType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="VehicleConfig" type="VehicleConfigType" minOccurs="0"/>
      <xsd:element name="OperationPerformed" type="xsd:string" minOccurs="0"/>
      <xsd:element ref="NotificationProgram" minOccurs="0"/>
      <xsd:element name="ResponseCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ResponseDesc" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServiceConfigType">
    <xsd:sequence>
      <xsd:element name="KeyValue" type="KeyValueType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServiceType">
    <xsd:sequence>
