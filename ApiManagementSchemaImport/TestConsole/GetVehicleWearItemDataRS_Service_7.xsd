  <xsd:element name="AccountVehicles" type="AccountVehiclesType"/>
  <xsd:complexType name="AccountVehiclesType">
    <xsd:complexContent>
      <xsd:extension base="ListElementType">
        <xsd:sequence>
          <xsd:element name="Vehicle" type="VehicleType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VehicleType">
    <xsd:annotation>
      <xsd:documentation>The Vehicle represents a vehicle that has or may
				have an OnStar unit installed.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="star:VehicleABIEType">
        <xsd:sequence>
          <xsd:element name="ManufacturerDescription" type="oagis:DescriptionType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Manufacturer description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="MakeDescription" type="oagis:DescriptionType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Make description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PhoneticName" type="PhoneticNameType" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="VehicleStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The list of valid STATUS_CD for a given
								VEHICLE. INACTIVE, ACTIVE, IN-SETUP, CANCELLED.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
