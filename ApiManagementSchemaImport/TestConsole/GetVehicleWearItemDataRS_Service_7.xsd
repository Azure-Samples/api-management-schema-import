            <xsd:annotation>
              <xsd:documentation>The Vehicle for Make, Model, Year, and
								Manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EventDateTime" type="EventDateTimeType" minOccurs="0" maxOccurs="unbounded">
