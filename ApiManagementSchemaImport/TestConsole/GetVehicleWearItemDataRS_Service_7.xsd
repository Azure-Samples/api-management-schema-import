      <xsd:element name="ChannelsEntitled" type="ChannelsEntitledType" minOccurs="0"/>
      <xsd:element name="InEligibleReason" type="InEligibleReasonType" minOccurs="0"/>
      <xsd:element name="ExpirationDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="ProductType" type="xsd:string" minOccurs="0"/>
      <xsd:element name="Category" type="CategoryType" minOccurs="0"/>
      <xsd:element name="Quantity" type="UsageType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CategoryType">
    <xsd:sequence>
      <xsd:element ref="Name"/>
