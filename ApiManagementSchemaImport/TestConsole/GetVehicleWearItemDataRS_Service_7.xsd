            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CurrentChargesAmount" type="oagis:AmountType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The sum of the charges incurred since the
								previous invoice.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CurrentTaxAmount" type="oagis:AmountType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The sum of the current taxes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CurrentAdjustmentsAmount" type="oagis:AmountType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The sum of the current adjustments.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CurrentPaymentsAmount" type="oagis:AmountType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The sum of the current payments.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DueDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The due date based on a configurable number of
								days after the invoice date.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="OrderID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Order ID.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="XMLInvoice" type="oagis:StringType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the Invoice document in XML format.</xsd:documentation>
            </xsd:annotation>
