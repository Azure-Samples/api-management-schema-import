      <xsd:element name="ProfileID" type="xsd:string" minOccurs="0"/>
      <xsd:element name="ServiceConfig" type="ServiceConfigType" minOccurs="0"/>
      <xsd:element name="OptInStatus" type="xsd:boolean"/>
      <xsd:element name="OptInDate" type="xsd:date" minOccurs="0"/>
      <xsd:element name="OptOutDate" type="xsd:date" minOccurs="0"/>
