  </xsd:complexType>
  <xsd:complexType name="MarketType">
    <xsd:sequence>
      <xsd:element name="Area" type="oagis:StringType" minOccurs="0"/>
      <xsd:element name="SubArea" type="oagis:StringType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AccountInfoType">
    <xsd:sequence>
      <xsd:element name="Market" type="MarketType" minOccurs="0"/>
      <xsd:element name="DataShareGroup" type="oagis:StringType" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="AuthorizedUsers" type="oagis:StringType" minOccurs="0"/>
      <xsd:element name="AvailableLinesToUse" type="oagis:StringType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="BillingAccount" type="BillingAccountType"/>
  <xsd:complexType name="BillingAccountHistoryInfoType">
    <xsd:sequence>
      <xsd:element ref="AccountID">
        <xsd:annotation>
          <xsd:documentation>The account number an OnStar account is
						identified by.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="VehicleID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Vehicle ID.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BalanceAmount" type="oagis:AmountType" nillable="false">
        <xsd:annotation>
          <xsd:documentation>The account balance.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AccountingDate" type="oagis:AccountingDateType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The effective date for this request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Payment" type="BillingAccountPaymentType" nillable="false">
        <xsd:annotation>
          <xsd:documentation>The Payment details.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="Order" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Order details.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Adjustment" type="BillingAdjustmentType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A billing adjustment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Invoice" type="BillingAccountInvoiceType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Price Plan.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="BillingAccountPaymentType">
    <xsd:complexContent>
      <xsd:extension base="BillingAccountPaymentBaseType">
        <xsd:sequence>
          <xsd:element name="ID" type="oagis:IdentifierType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The payment id.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Status" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Payment Status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BankID" type="oagis:IdentifierType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A unique identifier that identifies a bank.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BatchID" type="oagis:IdentifierType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A unique id assigned to check payments that
								are processed by banks in batches.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BatchItemNumber" type="oagis:LongNumericType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A sequential number assigned to items within a batch processed by
								a bank.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ChequeBatchDepositDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date the check was deposited by the bank.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ProcessDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The date the payment or refund is applied to
								the account balance.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="OnStarDebitCard" type="DebitCardType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The debit card payment method (extending
								type).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="BillingAccountPaymentBaseType">
    <xsd:complexContent>
      <xsd:extension base="oagis:PaymentType">
        <xsd:attribute name="type" type="oagis:StringType" use="optional">
          <xsd:annotation>
            <xsd:documentation>Indicates the type of nationality id (i.e. GovernmentID, Drivers License.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="BillingAccountType">
    <xsd:complexContent>
      <xsd:extension base="oagis:FinancialAccountType">
        <xsd:sequence>
          <xsd:element name="Status" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing account status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="StatusReason" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing account status reason.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="AccountPaymentMethod" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contains the payment method types and
								associated details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="PaymentMethod" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contains the payment method types and
								associated details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ReportingLevel" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing account reporting level. Indicates
								invoice or statement.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BillCycleDay" type="oagis:DayDateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing day of the month.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AccountingDate" type="oagis:AccountingDateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The effective date for this request.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BalanceAmount" type="oagis:AmountType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The account balance.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="FraudAlert" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The account balance.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="InvoiceFormat" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing invoice format.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Priority" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing priority.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Complexity" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing complexity.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Configuration" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing configuration.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Classification" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The billing classification.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="BillingAdjustment" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A billing adjustment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Invoice" type="BillingAccountInvoiceType" nillable="false" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The billing account invoices.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PaymentReceipt" type="BillingAccountPaymentReceiptType" nillable="false" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The billing account payment receipts.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AdditionalInfo" type="NameValueType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="PaymentMethod" type="AccountPaymentMethodType"/>
  <xsd:element name="AccountPaymentMethod" type="AccountPaymentMethodType"/>
  <xsd:complexType name="AccountPaymentMethodType">
    <xsd:annotation>
      <xsd:documentation>This entity holds the payment method details.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Type" type="NameCodeDescriptionType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>-A unique code for a given payment method type e.g., CC for Credit
						Card, CA for Checking Account.
						-The name of the payment method type
						-The text description of the payment method type</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CreditCard" type="CreditCardType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>credit card associated with the account</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DebitCard" type="DebitCardType" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The debit card payment method (extending type).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BankAccount" type="BankAccountType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>bank savings or checking account associated with
						the account</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CashPickup" type="CashPickupPaymentMethodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>cash pickup payment method.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PhoneBanking" type="PhoneBankingPaymentMethodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>phone banking payment method.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DebitContract" type="DebitContractPaymentMethodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>debit contract payment method.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrepaidCard" type="PrepaidCardPaymentMethodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>prepaid card payment method.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="WireMoneyTransfer" type="WireMoneyTransferPaymentMethodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>wire money transfer payment method.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExternalPayment" type="oagis:PaymentAuthorizationType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>external payment method Eg: Electronic transfer
						using web...</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
