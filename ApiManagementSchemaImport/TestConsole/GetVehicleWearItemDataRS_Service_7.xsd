  <xsd:complexType name="OrderInformationType">
    <xsd:complexContent>
      <xsd:extension base="SIDBusCu:CustomerOrder">
        <xsd:annotation>
          <xsd:documentation>The Order details.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="Payment" type="OrderPaymentMethodType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the payment method types and
								associated details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PaymentMethod" type="OrderPaymentMethodType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Contains the payment method types and
								associated details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PayerAccountID" type="oagis:IdentifierType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Payer Account ID associated with the
								order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="BundledProductOfferingID" type="oagis:IdentifierType" minOccurs="0"/>
          <xsd:element name="Characteristic" type="ProgramCharacteristicType" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="ProvisioningRequiredIndicator" type="oagis:IndicatorType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>An indicator used to determine whether or not
								any product on the order required provisioning before requesting
								payment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="LineItem" type="OrderLineItemType" nillable="false" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The Order Line Item Details.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UsePaymentMethodOnFileType" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates the file (GCC/Carrier) being
								used to fetch the payment information for this order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UsePaymentMethodOnFileIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if the credit card on files is being
								used for this order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IncludeAccountBalanceIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if the account balance is to be
								returned.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DealerInformation" minOccurs="0">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="PartyID" type="oagis:IdentifierType" minOccurs="0">
                  <xsd:annotation>
                    <xsd:documentation/>
                  </xsd:annotation>
                </xsd:element>
                <xsd:element name="DealerCode" type="oagis:CodeType" minOccurs="0">
                  <xsd:annotation>
                    <xsd:documentation/>
                  </xsd:annotation>
                </xsd:element>
                <xsd:element name="SellingSourceCode" type="oagis:CodeType" minOccurs="0">
                  <xsd:annotation>
                    <xsd:documentation/>
                  </xsd:annotation>
                </xsd:element>
                <xsd:element name="BusinessAssociateCode" type="oagis:CodeType" minOccurs="0">
                  <xsd:annotation>
                    <xsd:documentation/>
                  </xsd:annotation>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="EventDateTime" type="EventDateTimeType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The UFR and Cancel Dates.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Refund" type="RefundType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Order refund information</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UnsettledPaymentIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if there is an unsettled payment on
								the order.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="TransactionDetail" type="TransactionDetailType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Details related to an order transfer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="type" type="oagis:StringType" use="optional"/>
        <xsd:attribute name="action" type="oagis:StringType" use="optional"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
