      <xsd:element name="SegmentCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MessageCode" type="xsd:string" minOccurs="0"/>
      <xsd:element name="IsMarketable" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="CategorySeverity" type="xsd:string" minOccurs="0"/>
      <xsd:element name="CategoryPriority" type="xsd:string" minOccurs="0"/>
      <xsd:element name="KeyValue" type="KeyValueType" minOccurs="0" maxOccurs="unbounded"/>
