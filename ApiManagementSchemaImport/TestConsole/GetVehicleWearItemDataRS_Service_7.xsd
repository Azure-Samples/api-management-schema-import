  <xsd:complexType name="OdometerReadingType">
    <xsd:sequence>
      <xsd:element name="Value" type="oagis:MeasureType">
        <xsd:annotation>
          <xsd:documentation>Measure value for the time duration.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="RetailerType">
    <xsd:complexContent>
      <xsd:extension base="NameCodeDescriptionType">
        <xsd:sequence>
          <xsd:element name="TelephoneCommunication" type="oagis:CommunicationABIEType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The phone number(s) for the retailer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="Address" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Includes the physical and mailing address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VehicleLicenseType">
    <xsd:sequence>
      <xsd:element name="VehicleLicenseText" type="oagis:StringType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The license plate number of the vehicle.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StateOrProvinceCountrySub-DivisonID" type="oagis:IdentifierType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Unique identifier. (on ONS_VEHICLE this
						represents the government entity that has licensed the Vehicle
						License Plate.)In US, it is State</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="star:CountryID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation/>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="Carrier" type="CarrierOrganizationType"/>
  <xsd:complexType name="CarrierOrganizationType">
    <xsd:complexContent>
      <xsd:extension base="star:OrganizationABIEType">
        <xsd:sequence>
          <xsd:element name="CarrierCode" type="oagis:CodeType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The carrier code.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="NetworkCellularDeviceType">
    <xsd:annotation>
      <xsd:documentation>The cellular device type</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DeviceID" type="oagis:IdentifierType" nillable="false" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Device ID (MEID)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="NetworkCapabilityType">
    <xsd:annotation>
      <xsd:documentation>The type of a capability</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="NameValueType">
        <xsd:sequence>
          <xsd:element name="AvailableIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The available indicator for the network
								capability.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MobileNumberRegistrationType">
    <xsd:sequence>
      <xsd:element ref="Carrier" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The carrier organization.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SID" type="oagis:IdentifierType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>SID.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EventDateTime" type="EventDateTimeType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Registration and expiration Datetimes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OnStarProgramType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element ref="AccountStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Account Status Optional, utilized for program eligibility</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="VehicleStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Vehicle Status Optional, utilized for program eligibility</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="VehicleProgram" type="VehicleProgramType" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="SubscriberProgram" type="SubscriberProgramType" minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element ref="VehicleID" minOccurs="0"/>
          <xsd:element ref="AccountID" minOccurs="0"/>
          <xsd:element ref="SubscriberID" minOccurs="0"/>
          <xsd:element name="OnStarProgramInfoList" type="OnStarProgramInfoListType" minOccurs="0"/>
          <xsd:element name="Product" type="NewProductType" minOccurs="0"/>
          <xsd:element name="Cancellation" type="CancellationType" minOccurs="0"/>
          <xsd:element name="Order" type="OrderType" minOccurs="0"/>
          <xsd:element name="LastSubscriberFlag" type="xsd:boolean" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Cancellation" type="CancellationType"/>
  <xsd:complexType name="CancellationType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <!--Key-->
          <xsd:element ref="VehicleID"/>
          <xsd:element ref="AccountID" minOccurs="0"/>
          <xsd:element ref="StationID" minOccurs="0"/>
          <!--<xsd:element ref="eac:VehicleUnit" minOccurs="1" maxOccurs="1" nillable="false"/>
					<xsd:element ref="eac:Account" minOccurs="0" maxOccurs="1" nillable="false"/>
					<xsd:element ref="eac:VehicleProfile" minOccurs="0" maxOccurs="1" nillable="false"/>
					<xsd:element ref="eac:Product" minOccurs="0" maxOccurs="unbounded" nillable="false"/>-->
          <xsd:element name="UnitStatus" type="oagis:StringType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Unit State</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="EligibilityType" type="oagis:StringType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Possible values could be Deactivation,NotEligible,Connected</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="InEligibleReason" type="oagis:StringType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The reason for not being eligible for deactivation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DeactivationDate" type="oagis:DateType" nillable="true" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Returns the future date for deactivation.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AdditionalInfo" type="NameValueType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contains additional information that might need to be passed</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="OnStarProgramInfoListType">
    <xsd:choice minOccurs="0" maxOccurs="unbounded">
      <xsd:element ref="OnStarProgramInfo" minOccurs="0"/>
      <xsd:element ref="VehicleProgramInfo" minOccurs="0"/>
      <xsd:element ref="SubscriberProgramInfo" minOccurs="0"/>
      <xsd:element ref="InsuranceCompanyDiscountProgramInfo" minOccurs="0"/>
      <xsd:element ref="DMNProgramInfo" minOccurs="0"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:element name="SubscriberProgramInfo" type="SubscriberProgramInfoType"/>
  <xsd:complexType name="SubscriberProgramInfoType">
    <xsd:annotation>
      <xsd:documentation>The Subscriber Program information.  Includes the OVD Program.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OnStarProgramInfoType"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="InsuranceCompanyDiscountProgramInfo" type="InsuranceCompanyDiscountProgramInfoType"/>
  <xsd:element name="DMNProgramInfo" type="DMNProgramInfoType"/>
  <xsd:complexType name="DMNProgramInfoType">
    <xsd:annotation>
      <xsd:documentation>The Insurance Company Discount gram information.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OnStarProgramInfoType">
        <xsd:sequence>
          <xsd:element name="DataSharingDealer" type="DealerType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Dealer that the subscriber has indicated to receive data about their vehicle from OnStar.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="VehicleProgramInfo" type="VehicleProgramInfoType"/>
  <xsd:complexType name="VehicleProgramInfoType">
    <xsd:annotation>
      <xsd:documentation>The Vehicle Program information.  Includes the OVD Program.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OnStarProgramInfoType">
        <xsd:sequence>
          <xsd:element name="TriggerPreferenceInfo" type="TriggerPreferenceType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Contains the trigger preferences.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NextFireDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>...</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="LastFireDate" type="oagis:DateType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>...</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!--Start of New Features Proposal-->
  <xsd:element name="ID" type="xsd:string"/>
  <xsd:element name="Name" type="xsd:string"/>
  <xsd:element name="Description" type="xsd:string"/>
  <xsd:element name="Code" type="xsd:string"/>
