  <xsd:element name="PhoneticName" type="PhoneticNameType"/>
  <xsd:complexType name="PhoneticNameType">
    <xsd:sequence>
      <xsd:element name="LanguageCode" type="oagis:LanguageCodeType"/>
      <xsd:element name="Name" type="oagis:NameType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="VehicleStatus" type="NameCodeDescriptionType">
    <xsd:annotation>
      <xsd:documentation>- Code: The code indicating the status of account
				billing
				- Name: The word or words that an account billing status type
				is known by. Examples include: currently paid, written off, past
				due, credit card pending, credit card denied, normal.
				- Description:
				Additional information about an status type.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="CarrierAccountType">
    <xsd:complexContent>
      <xsd:extension base="BaseNounType">
        <xsd:sequence>
          <xsd:element ref="AccountID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Carrier Billing Account Number or BAN</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="VehicleID" minOccurs="0"/>
          <xsd:element ref="AccountType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The carrier account type.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="AccountStatus" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The carrier account status.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Carrier" type="NameCodeDescriptionType" nillable="false" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The carrier identifier</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AccountInfo" type="AccountInfoType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The carrier plan details</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Address" type="AddressType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The Address from Carrier</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Subscriber" type="SubscriberType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Subscriber details from Carrier.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AdditionalInfo" type="NameValueType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
