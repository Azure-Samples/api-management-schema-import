            <xsd:annotation>
              <xsd:documentation>Outstanding Balance Amount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="OrderInformation" minOccurs="0" maxOccurs="unbounded"/>
