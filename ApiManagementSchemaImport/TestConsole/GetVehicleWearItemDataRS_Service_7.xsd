            <xsd:annotation>
              <xsd:documentation>Contains Order Status information</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AdditionalInfo" type="NameValueType" nillable="true" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
