    <xsd:annotation>
      <xsd:documentation>Features (Services)</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="ID" minOccurs="0"/>
      <xsd:element ref="Name" minOccurs="0"/>
      <xsd:element ref="Description" minOccurs="0"/>
