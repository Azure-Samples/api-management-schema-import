              <xsd:documentation>The UFR and Cancel Dates.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="GarageAddress" type="AddressType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The Garage Address of the Vehicle</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
