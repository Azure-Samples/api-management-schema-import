        <xsd:annotation>
          <xsd:documentation>meter per second (m/s)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="TimeUnitsContentType">
