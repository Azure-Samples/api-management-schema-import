          <xsd:element name="NewUnitState" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Valid Values: UNIT_STATE_UNKNOWN,UNIT_STATE_ENABLED,UNIT_STATE_DISABLED,UNIT_STATE_NOT_CONFIGURED</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewPersonalCallingState" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Valid Values: PERSONAL_CALLING_STATE_UNKNOWN,PERSONAL_CALLING_STATE_ENABLED_PREPAY,PERSONAL_CALLING_STATE_ENABLED_POSTPAY,PERSONAL_CALLING_STATE_DISABLED,PERSONAL_CALLING_STATE_NOT_CONFIGURED</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewVDUState" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Valid Values: VDU_STATE_UNKNOWN,VDU_STATE_ENABLED,VDU_STATE_DISABLED,VDU_STATE_NOT_CONFIGURED</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewVirtualAdvisorState" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Valid Values: VIRTUAL_ADVISOR_STATE_UNKNOWN, VIRTUAL_ADVISOR_STATE_ENABLED_PREPAY, VIRTUAL_ADVISOR_STATE_ENABLED_POSTPAY, VIRTUAL_ADVISOR_STATE_DISABLED, VIRTUAL_ADVISOR_STATE_NOT_CONFIGURED</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <!--For Updating the langauge-->
          <xsd:element name="TTYIndicator" type="oagis:IndicatorType" minOccurs="0"/>
          <!--MR01 - change-->
          <xsd:element name="NewVehicleTheftState" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Valid Values: VEHICLE_THEFT_STATE_UNKNOWN,VEHICLE_THEFT_STATE_ENABLED,VEHICLE_THEFT_STATE_DISABLED,VEHICLE_THEFT_STATE_NOT_CONFIGURED</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewContentTheftState" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Valid Values: CONTENT_THEFT_STATE_UNKNOWN,CONTENT_THEFT_STATE_ENABLED,CONTENT_THEFT_STATE_DISABLED,CONTENT_THEFT_STATE_NOT_CONFIGURED</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="ForceAllConfigFlag" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>true or false to indicate whether all unit states should be forcefuly reconfigured.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="PreferredLanguageCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Subscriber preferred language code</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <!--No longer a possible options for Activate Unit
					<xsd:element ref="ForceVDUConfigFlag" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>true or false to indicate whether all triggers should be forcefuly refreshed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>-->
          <!--Mobile Number Change request data-->
          <xsd:element ref="NetworkNumber" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="DialableNumber" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="DeviceID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="ICCID" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewCarrier" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="CDMAHomeSIDData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="GSMHomeSIDData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="LTEHomeSIDData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="CDMAPacketDataRegistrationData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="GSMPacketDataRegistrationData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="LTEPacketDataRegistrationData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="UseNewDialableNumber" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates if the new MDN is to be used for dialing</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <!--PCU request management request data-->
          <xsd:element ref="PCUManagementData" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IntermediateRespRequiredAfterBaseConfigIndicator" type="oagis:IndicatorType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
          <xsd:element ref="HomeArea" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The subscriber home area address details to determine the cellular network home area..</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <!--Gen9 - change-->
          <xsd:element name="NewAIFSecurityMode" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>ValidValues: "CERIFICATE", "NONE", "SHARED"</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewPacketDataSecurityMode" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>ValidValues: "CERIFICATE", "NONE", "SHARED"</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewSMSSecurityMode" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>ValidValues: "CERIFICATE", "NONE", "SHARED"</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NewTBTSecurityMode" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>ValidValues: "CERIFICATE", "NONE", "SHARED"</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <!--Additional information for processing-->
          <xsd:element name="AdditionalParameter" type="eac:NameValueType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The unit capabilities.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <!--Family link changes-->
          <xsd:element name="NewTrafficProbeState" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
