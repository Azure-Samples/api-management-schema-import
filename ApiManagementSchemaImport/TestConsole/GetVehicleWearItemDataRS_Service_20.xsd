            </xsd:annotation>
          </xsd:element>
          <xsd:element name="AlertOverride" type="oagis:StringType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Indicates which default lights Alert behavior is selected. Valid Values: OVERRIDE_NONE, OVERRIDE_DEFAULT, OVERRIDE_DOOR_OPEN, OVERRIDE_IGNITION_ON</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
